 <header class="page-header">
    <h1> New Category</h1>
  </header>


<%= form_for [:admin, @category] do |form|%>
  <div class="mb-3">
    <%= form.label :name, class: 'form-label'%>
    <%= form.text_field :name, class: 'form-control'%>
  </div>
  <br>
  <%= form.submit 'Create Product', class: 'btn btn-primary' %>
<% end %>
